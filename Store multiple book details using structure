#include <stdio.h>
struct book {
    int id;
    char title[20];
    float price;
};
int main() {
    struct book b[3];
    int i;
    for (i = 0; i < 3; i++)
        scanf("%d %s %f", &b[i].id, b[i].title, &b[i].price);
    for (i = 0; i < 3; i++)
        printf("%d %s %.2f\n", b[i].id, b[i].title, b[i].price);
    return 0;
}
