#include <stdio.h>

int main() {
    int cost, selling;

    printf("Enter Cost Price: ");
    scanf("%d", &cost);

    printf("Enter Selling Price: ");
    scanf("%d", &selling);

    if (selling > cost)
        printf("Profit = %d", selling - cost);
    else if (cost > selling)
        printf("Loss = %d", cost - selling);
    else
        printf("No Profit No Loss");

    return 0;
}
