#include <stdio.h>
struct customer {
    int id;
    char name[20];
    char mobile[15];
};
int main() {
    struct customer c[3];
    int i, key;
    for (i = 0; i < 3; i++)
        scanf("%d %s %s", &c[i].id, c[i].name, c[i].mobile);
    scanf("%d", &key);
    for (i = 0; i < 3; i++)
        if (c[i].id == key)
            printf("%d %s %s", c[i].id, c[i].name, c[i].mobile);
    return 0;
}
